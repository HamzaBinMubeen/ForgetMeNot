<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                    android:width="1dp"
                    android:color="#1F000000" />
        </shape>
    </item>
    <item
            android:bottom="12.5dp"
            android:left="9.5dp"
            android:right="9.5dp"
            android:top="12.5dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
            <size
                    android:width="8dp"
                    android:height="2dp" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item
            android:bottom="9.5dp"
            android:left="12.5dp"
            android:right="12.5dp"
            android:top="9.5dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
            <size
                    android:width="2dp"
                    android:height="8dp" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</layer-list>