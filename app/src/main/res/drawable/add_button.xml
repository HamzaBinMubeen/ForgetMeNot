<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/colorAccent" />
        </shape>
    </item>
    <item
            android:bottom="13dp"
            android:left="10dp"
            android:right="10dp"
            android:top="13dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <size
                    android:width="8dp"
                    android:height="2dp" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item
            android:bottom="10dp"
            android:left="13dp"
            android:right="13dp"
            android:top="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <size
                    android:width="2dp"
                    android:height="8dp" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</layer-list>