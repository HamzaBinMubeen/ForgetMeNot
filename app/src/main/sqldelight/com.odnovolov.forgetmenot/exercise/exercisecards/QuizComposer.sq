exerciseCardWithoutQuiz:
SELECT TempExerciseCard.id AS exerciseCardId,
       TempExerciseCard.cardId,
       Card.deckId,
       Card.answer
FROM TempExerciseCard JOIN Card ON TempExerciseCard.cardId == Card.id
WHERE testMethod == 2 AND TempExerciseCard.id NOT IN (SELECT exerciseCardId FROM TempQuiz);

getCardsByDeckIds:
SELECT * FROM Card WHERE deckId IN ?;

addQuiz:
